#
# Generated by Chef for <%= node[:hostname] %>
#
God.load "/etc/god/conf.d/*.god"

God::Contacts::Email.defaults do |d|
  d.from_email = <%= node[:god][:from_email].inspect %>
  d.to_email = <%= node[:god][:to_email].inspect %>
  d.delivery_method = <%= node[:god][:email_delivery_method].inspect %>
  d.server_host = <%= node[:god][:email_server_host].inspect %>
  d.server_port = <%= node[:god][:email_server_port].inspect %>
end
